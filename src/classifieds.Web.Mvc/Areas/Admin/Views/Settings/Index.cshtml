@using classifieds.Web.Startup@using Abp.Configuration;@model IList<ISettingValue>@{
	ViewBag.Title = L("Settings");
	ViewBag.CurrentPageName = PageNames.Settings;}<div class="content">	<div class="container-fluid">		<br />		<div class="row">			<div class="col-12">				<div class="card">					<div class="card-header">Settings</div>					<div class="card-body">						<table class="table">							<thead>								<tr>									<th>Name</th>									<th>Value</th>									<th></th>								</tr>							</thead>							<tbody>								@foreach (var item in Model)
								{									<tr>										<td>@item.Name.Replace("."," ")</td>										<td>@item.Value</td>										<td><a class="btn btn-primary" href="/admin/settings/edit?name=@item.Name" target="blank">Edit</a></td>									</tr>
								}							</tbody>						</table>					</div>				</div>			</div>		</div>	</div></div>